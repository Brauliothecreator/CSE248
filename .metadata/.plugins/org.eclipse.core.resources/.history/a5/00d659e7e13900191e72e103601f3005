
public class NameBag {
	private Name[] names;
	private int nElems;
	private int maxSize;
	
	
	public NameBag(int maxSize) {
		names = new Name[maxSize];
		nElems = 0;
		this.maxSize = maxSize;
		
		
	}
	public int getMaxSize() {
		return maxSize;
	}
	public void setMaxSize(int maxSize) {
		this.maxSize = maxSize;
	}
	public void insert(Name name) {
		names[nElems++] = name;
	}
	
	public void searchByGender(char gender) {
		
	}
	public void searchByFirstLetter(char letter) {
		int x = 0;
		while( x != maxSize) {
			if(names[x].getFirstName().charAt(0) == letter);
			
		}
		
	}
	public void display() {
		for(int i = 0; i < nElems; i++) {
			System.out.print(names[i]);
		}
		System.out.println();
	}
}
