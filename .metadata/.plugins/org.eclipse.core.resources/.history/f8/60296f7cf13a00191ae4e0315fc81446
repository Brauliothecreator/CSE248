package accounts;

import Names.NameWarehouse;

public class Demo {

	public static void main(String[] args) {
		NameWarehouse nw = new NameWarehouse();
		UserAccountFactory f = new UserAccountFactory(nw);
		System.out.println("\n " + f.emitFirstName());
		System.out.println(f.emitLastName());
		System.out.println(f.emitGender());
		f.emitUserAccount();
		System.out.println("\n " + f.emitFirstName());
		System.out.println(f.emitLastName());
		System.out.println(f.emitGender());
		f.emitUserAccount();
		System.out.println("\n " + f.emitFirstName());
		System.out.println(f.emitLastName());
		System.out.println(f.emitGender());
		f.emitUserAccount();
		System.out.println("\n " + f.emitFirstName());
		System.out.println(f.emitLastName());
		System.out.println(f.emitGender());
		f.emitUserAccount();
		System.out.println("\n " + f.emitFirstName());
		System.out.println(f.emitLastName());
		System.out.println(f.emitGender());
		f.getAccountBag().displayAccounts();
	}

}
